<body>
<%= form_for @concert do |f| %>
  <p>
    <%= f.label :band_name %><br>
    <%= f.text_field :band_name %>
  </p>

  <p>
    <%= f.label :venue %><br>
    <%= f.text_area :venue %>
  </p>
 <p>
    <%= f.label :city %><br>
    <%= f.text_area :city %>
  </p>
 <p>
    <%= f.label :date %><br>
    <%= f.text_area :date %>
  </p>
   <p>
    <%= f.label :price %><br>
    <%= f.text_area :price %>
  </p>
 <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>


  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', concerts_path %>
</body>